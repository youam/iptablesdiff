use strict;
use warnings;

my %strings;
while (<>) {
	chomp;
	next if /^$/;
	if ( m/^([a-zA-Z0-9._+-]+)\s+(.*)$/ ) {
		my $k = $1;
		my $v = $2;
		push @{$strings{$k}}, $v;
	} else {
		die "did not match: '$_'";
	}
}

for my $k ( sort keys %strings ) {
#	next if scalar @{$strings{$k}} == 1;
	print $k," ", join( " ", sort @{$strings{$k}}),"\n";
}
